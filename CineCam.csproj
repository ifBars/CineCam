		<ImplicitUsings>enable</ImplicitUsings>
		<Nullable>disable</Nullable>
		<RootNamespace>CineCam</RootNamespace>
		<LangVersion>default</LangVersion>
		<IsPackable>false</IsPackable>
		<NeutralLanguage>en-US</NeutralLanguage>
		<AssemblyName>CineCam</AssemblyName>
		<LangVersion>latest</LangVersion>
		<ImplicitUsings>enable</ImplicitUsings>
		<Configurations>Mono;Il2cpp</Configurations>
	</PropertyGroup>

	<!-- Extract version from VersionInfo.cs at build time -->
	<Target Name="GetVersionFromSource" BeforeTargets="GetAssemblyVersion;GetAssemblyFileVersion;CoreCompile">
		<PropertyGroup>
			<VersionInfoPath>$(MSBuildProjectDirectory)\VersionInfo.cs</VersionInfoPath>
			<VersionInfoContent>$([System.IO.File]::ReadAllText('$(VersionInfoPath)'))</VersionInfoContent>
			<VersionRaw>
				$([System.Text.RegularExpressions.Regex]::Match(
				'$(VersionInfoContent)',
				'VERSION_STRING\s*=\s*"([0-9]+\.[0-9]+\.[0-9]+)"'
				).Groups[1].Value)
			</VersionRaw>
			<VersionFromSource>$(VersionRaw.Trim())</VersionFromSource>
			<AssemblyVersion>$(VersionFromSource)</AssemblyVersion>
			<FileVersion>$(VersionFromSource)</FileVersion>
		</PropertyGroup>

		<Message Text="Using version from VersionInfo.cs: $(VersionFromSource)" Importance="high" />
	</Target>

	<PropertyGroup Condition="'$(Configuration)' == 'Mono'">
		<DefineConstants>$(DefineConstants);MONO</DefineConstants>
		<AssemblyName>CineCam_Mono</AssemblyName>
	</PropertyGroup>

	<!-- PropertyGroup for the IL2CPP configuration -->
	<PropertyGroup Condition="'$(Configuration)' == 'Il2cpp'">
